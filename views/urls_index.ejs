<!DOCTYPE html>

<html lang="en">

<head>
  <title>TinyApp</title>
</head>

<body>
    <% include ./partials/_header %>

    <ul>
      <% for (var url in urls) { %>
      <form method="POST" action="/urls/<%= url %>/delete">
      <li><a href="/urls/<%= url %>"><%= url %> - <%= urls[url] %></a></li>
        <input type="submit" value="DELETE">
      </form>
      <% } %>
    </ul>

</body>

</html>