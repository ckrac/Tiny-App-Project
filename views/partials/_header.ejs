<h1>TinyApp</h1>

  <% if (user_id) { %>
  <h1>Logged in already</h1>
  <h3>user_id: <%= user_id %></h3>
  <form action="/login/logout" method="POST">
    <input type="submit" value="Logout">
  </form>
  <% } else { %>
<h3>Login: <a href="/login">click me</a></h3>
<h3>Register as a new user: <a href="/register">click me</a></h3>
  <% } %>
